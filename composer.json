{
    "name": "azima/laravel-esl",
    "description": "A Laravel package for Azima ESL connection.",
    "keywords": [
        "esl",
        "Azima",
        "event socket",
        "gsm"
    ],
    "homepage": "https://github.com/Mahmoudalziem/Azima-larave-esl",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Md Mostafijur Rahman",
            "email": "mostofa122@gmail.com",
            "homepage": "https://mostofa.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Azima\\LaravelEsl\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Azima\\LaravelEsl\\LaravelEslServiceProvider"
            ],
            "aliases": {
                "Esl": "Azima\\LaravelEsl\\Facades\\Esl"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}